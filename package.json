{
  "name": "amelia",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://hugohabel.github.io/amelia-start-page",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@storybook/addon-docs": "^6.4.20",
    "dayjs": "^1.11.0",
    "formik": "^2.2.9",
    "gh-pages": "^3.2.3",
    "immer": "^9.0.5",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "deploy": "gh-pages -d build -r https://github.com/hugohabel/amelia-start-page.git",
    "eject": "react-scripts eject",
    "format": "prettier \"**/*.{js,json,md,ts,tsx,html}\" --write",
    "lint": "eslint --ext js,ts,tsx,html . && tsc --pretty --noEmit",
    "predeploy": "react-scripts build",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "test-local": "react-scripts test --watchAll --coverage --collectCoverageFrom=!src/common/contexts/AppState.tsx --collectCoverageFrom=!src/common/state/state.ts --collectCoverageFrom=!src/common/types/AppState.ts --collectCoverageFrom=!src/**/tests/__snapshots__/* --collectCoverageFrom=!src/__snapshots__/* --collectCoverageFrom=!src/index.tsx --collectCoverageFrom=!src/reportWebVitals.ts",
    "test": "TZ='America/Panama' react-scripts test --watchAll=false",
    "update-snapshots": "TZ='America/Panama' react-scripts test -u --watchAll=false"
  },
  "pre-commit": [
    "format",
    "lint"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "0.0.9",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^8.3.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "webpack": "^5.72.0"
  }
}
